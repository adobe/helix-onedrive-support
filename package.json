{
  "name": "@adobe/helix-onedrive-support",
  "version": "12.1.3",
  "description": "Helix OneDrive Support",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "type": "module",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "c8 mocha --spec=test/**/*.test.js",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --branches $CI_BRANCH",
    "docs": "npx jsdoc2md -c .jsdoc.json --files 'src/*.js'  > docs/API.md",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-onedrive-support.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-onedrive-support/issues"
  },
  "homepage": "https://github.com/adobe/helix-onedrive-support#readme",
  "dependencies": {
    "@adobe/fetch": "^4.1.10",
    "@adobe/helix-shared-string": "^2.1.0",
    "@adobe/helix-shared-tokencache": "^1.4.19",
    "@azure/msal-node": "3.8.2",
    "jose": "6.1.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.14",
    "@aws-sdk/client-s3": "3.933.0",
    "@eslint/config-helpers": "0.5.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "ajv": "8.17.1",
    "c8": "10.1.3",
    "chalk-template": "1.1.2",
    "dotenv": "17.2.3",
    "eslint": "9.4.0",
    "husky": "9.1.7",
    "install": "0.13.0",
    "jsdoc-to-markdown": "9.1.3",
    "jsdoc-tsimport-plugin": "1.0.5",
    "junit-report-builder": "5.1.1",
    "lint-staged": "16.2.6",
    "mocha": "11.7.5",
    "mocha-multi-reporters": "1.5.1",
    "mocha-suppress-logs": "0.6.0",
    "nock": "13.5.6",
    "npm": "11.6.2",
    "semantic-release": "25.0.2"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "mocha": {
    "require": [
      "test/setup-env.js",
      "mocha-suppress-logs"
    ],
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  }
